<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/DECORATION/1.6.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/DECORATION/1.7.0 http://maven.apache.org/xsd/decoration-1.7.0.xsd"
	name="Decima Content Validation Framework">
	<bannerLeft>
		<!-- Reflow Maven Skin, but with "Reflow" highlighted -->
		<name><![CDATA[
      <span class="nist-logo" title="Go to nist.gov">National Institute of
            Standards and Technology</span>
      ]]>
		</name>
		<href>http://www.nist.gov/</href>
	</bannerLeft>
	<publishDate position="bottom" format="yyyy-MM-dd" />
	<version position="bottom" />

	<skin>
		<groupId>lt.velykis.maven.skins</groupId>
		<artifactId>reflow-maven-skin</artifactId>
		<version>1.1.1</version>
	</skin>
	<custom>
		<reflowSkin>
			<theme>bootswatch-spacelab</theme>
			<!-- <highlightJs>true</highlightJs> -->
			<brand>
				<name>
	        <![CDATA[
	        <span class="nist-logo" title="Go to nist.gov">National Institute of
            Standards and Technology</span>
	        ]]>
				</name>
				<href>http://www.nist.gov/</href>
			</brand>
			<titleTemplate>%2$s | %1$s</titleTemplate>
			<toc>top</toc>
			<topNav>Download|reports|modules</topNav>
			<bottomNav>
				<column>Main|Download</column>
				<column>Documentation</column>
				<column>reports|modules</column>
			</bottomNav>
			<bottomDescription>
				This is a very good project doing interesting and
				valuable things.
			</bottomDescription>
			<pages>
				<index>
					<shortTitle>Welcome</shortTitle>
					<breadcrumbs>false</breadcrumbs>
					<toc>false</toc>
					<sections>
						<carousel />
						<body />
						<sidebar />
						<thumbs>2</thumbs>
						<columns>3</columns>
					</sections>
				</index>
				<developer-info>
					<toc>sidebar</toc>
				</developer-info>
			</pages>
		</reflowSkin>
	</custom>
	<body>
		<menu name="Overview">
			<item name="Introduction" href="index.html" />
		</menu>
		<menu ref="modules" />
		<menu ref="reports" />
	</body>
</project>